{"ast":null,"code":"import { SET_FILTER } from '../actions/searchFilterActions';\nimport { requestTransactionList, resetTransactionList } from '../actions/transactionPackActions';\nexport default (store => nextRunner => action => {\n  //검색 필터 변경이 생길때 자동으로 검색결과 요청을 구현\n  const {\n    type,\n    payload\n  } = action;\n  const result = nextRunner(action);\n\n  if (type === SET_FILTER) {\n    const {\n      params\n    } = payload || {};\n    store.dispatch(resetTransactionList()); //검색결과를 요청하기 전에 현재 검색 목록 초기화\n\n    store.dispatch(requestTransactionList(params));\n  }\n\n  return result;\n});","map":null,"metadata":{},"sourceType":"module"}